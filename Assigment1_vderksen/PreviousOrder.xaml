<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    Title="Transaction History"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Assigment1_vderksen.PreviousOrder">

    <ContentPage.Content>
         <StackLayout>
            <ListView x:Name="ordersList" HasUnevenRows="True">
            <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding orderDate}" Detail="{Binding orderSum}">
                </TextCell>
            </DataTemplate>
            </ListView.ItemTemplate>
       </ListView>
         </StackLayout>
    </ContentPage.Content>
</ContentPage>
